{
  "projects": {
    "default": "rentfax-revamp"
  },
  "hosting": [
    {
      "site": "rentfax-revamp",
      "public": "out",
      "source": ".",
      "frameworksBackend": {
        "region": "us-central1"
      },
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|woff|woff2|ttf|eot|ico)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "**",
          "headers": [
            {
              "key": "Strict-Transport-Security",
              "value": "max-age=31536000; includeSubDomains; preload"
            },
            {
              "key": "X-Frame-Options",
              "value": "DENY"
            },
            {
              "key": "X-Content-Type-Options",
              "value": "nosniff"
            },
            {
              "key": "X-XSS-Protection",
              "value": "1; mode=block"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "rentfax-demo",
      "public": "out",
      "source": ".",
      "frameworksBackend": {
        "region": "us-central1"
      }
    },
    {
      "site": "rentfax-staging",
      "public": "out",
      "source": ".",
      "frameworksBackend": {
        "region": "us-central1"
      }
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
